{
  "version": 2,
  "buildCommand": "bash ./build.sh",
  "outputDirectory": "dist",
  "installCommand": "npm install",
  "framework": null,
  "regions": ["iad1"],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "^/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}